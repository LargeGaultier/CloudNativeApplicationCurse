    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.7.4",
    "prisma": "^5.6.0",
    "supertest": "^7.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
