{
  "name": "gym-management-backend",
  "version": "1.0.0",
  "description": "Backend for gym management system",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
